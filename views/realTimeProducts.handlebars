<script src="https://cdn.socket.io/4.7.5/socket.io.min.js" integrity="sha384-2huaZvOR9iDzHqslqwpR87isEmrfxqyWOF7hr7BY6KG0+hVKLoEXMPUJw3ynWuhO" crossorigin="anonymous"></script>
<script src="/js/index.js"></script>
<div>
    <h3 class="mt-3" align="center">Formulario y tabla en handlebars con WebSokets</h3>

    {{!-- <h6 align="center">Formulario para guardar productos</h6> --}}

    <div class="center-text" align="center">
        <form>
            <div class="form-row">
                <div class="form-group col-md-6 mb-2">
                    <input type="text" class="form-control" id="title" placeholder="Titulo">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="text" class="form-control" id="description" placeholder="Description">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="text" class="form-control" id="code" placeholder="code">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="number" class="form-control" id="price" placeholder="price">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="text" class="form-control" id="status" placeholder="status">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="number" class="form-control" id="stock" placeholder="stock">
                </div>
                <div class="form-group col-md-6 mb-2">
                    <input type="text" class="form-control" id="category" placeholder="category">
                </div>
            </div>
        <button type="button" class="btn btn-primary" onclick="guardar()">Save</button>
        </form>
    </div>

